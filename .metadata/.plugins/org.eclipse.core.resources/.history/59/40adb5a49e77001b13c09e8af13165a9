public class TesteElevador {

	public static void main(String[] args) {

		Elevador elevador = new Elevador();

		elevador.configurar((short)10, (short)0, (byte)20);
		//ou
		/*elevador.configurar(
		 * (short)10,
		 * (short)0,
			* (byte)20);
		 * 
		 */
		elevador.preencherNome("Cai Cai Elevador");
